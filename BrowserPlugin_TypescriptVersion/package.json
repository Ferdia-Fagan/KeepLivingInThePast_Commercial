{
  "name": "keep-living-in-the-past-man",
  "version": "1.0.0",
  "description": "browser plugin",
  "main": "index.js",
  "scripts": {
    "watch": "webpack --watch",
    "start:firefox": "web-ext run --source-dir ./dist",
    "build:dev": "webpack --mode=development && web-ext build --source-dir ./dist --overwrite-dest",
    "build:prod": "webpack --mode=production && web-ext build --source-dir ./dist --overwrite-dest",

    "prestart": "npm run build",
    "start": "npm run build",
    "lint": "eslint . --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@types/webextension-polyfill": "^0.8.0"
  },
  "devDependencies": {
    "@types/node": "^16.7.10",
    "@types/webextension-polyfill": "^0.8.0",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "awesome-typescript-loader": "^5.2.1",
    "eslint": "^7.32.0",
    "source-map-loader": "^3.0.0",
    "typescript": "^3.2.0",
    "web-ext": "^6.3.0",
    "web-ext-types": "^3.2.1",
    "webextension-polyfill": "^0.8.0",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0"
  },
  "author": "Ferdia Fagan",
  "license": "MIT"
}
