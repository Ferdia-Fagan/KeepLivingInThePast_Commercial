{
  "name": "keep-living-in-the-past-man",
  "version": "1.0.0",
  "description": "browser plugin",
  "main": "index.js",
  "scripts": {
    "watch": "webpack --watch",
    "start:firefox": "web-ext run --source-dir ./extension/src",
    "build:dev": "tsc",
    "build:prod": "webpack --mode=production && web-ext build --source-dir ./extension/src --overwrite-dest",
    "prestart": "npm run build",
    "start": "npm run build",
    "lint": "eslint . --ext .ts",
    "test": "jest --verbose",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "page-metadata-parser": "^1.1.4"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.17",
    "@types/webextension-polyfill": "^0.8.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "awesome-typescript-loader": "^5.2.1",
    "eslint": "^7.32.0",
    "fake-indexeddb": "^3.1.7",
    "jest": "^27.4.5",
    "mockzilla": "^0.12.0",
    "mockzilla-webextension": "^0.13.0",
    "source-map-loader": "^3.0.0",
    "ts-jest": "^27.1.2",
    "typescript": "^3.2.0",
    "web-ext": "^6.6.0",
    "web-ext-types": "^3.2.1",
    "webextension-polyfill": "^0.8.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "author": "Ferdia Fagan",
  "license": "MIT"
}
